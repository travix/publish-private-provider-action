{
  "name": "publish-private-provider-action",
  "version": "0.0.0",
  "private": true,
  "description": "Publishes a private terraform provider generated by goreleaser to the Terraform Cloud",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "all": "npm run format && npm run lint -- --fix && npm test -- --run && npm run build",
    "prebuild": "rm -rf dist",
    "build": "ncc build ./src/main.ts -mo dist --source-map --license licenses.txt --target es2020",
    "format": "prettier --write '**/*.{ts,js,cjs,json}'",
    "format-check": "prettier --check '**/*.{ts,js,cjs,json}'",
    "lint": "eslint '**/*.ts' --cache",
    "test": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travix/publish-private-provider-action.git"
  },
  "keywords": [
    "actions",
    "terraform",
    "private",
    "provider"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.2",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.60.1",
    "@vercel/ncc": "^0.36.1",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "got": "^13.0.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "semver": "^7.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.32.2"
  }
}
